<h1><%= @client.vorname %> <%= @client.name %> <%= @client.patronymic %></h1>

  <div class="form-group row-fluid">
    <div class="span3"><p>Телефон <%= @client.phone %></p></div>
    <div class="span4"><p> <%= @client.email %></p></div>
  </div>
  <div class="form-group row-fluid">
    <div class="span3"><p>Дата первого обращения <%= @client.date_in %></p></div>
    <div class="span4"><p> <%= @client.comment %></p></div>
  </div>
<hr></hr>
<ul class="passports">
  <% @technpassports.each do |technpassport| %>
    <div class="form-group row-fluid">
      <div class="span2"><p>Дата <b><%= technpassport.date_in %></b></p></div>
      <div class="span1"><p>№ <%= link_to technpassport.id, :controller => :technpassports, :action => :edit, :id => technpassport.id %></p></div>
      <div class="span2"><p>Сумма <b><%= technpassport.amount %></b></p></div>
      <div class="span2"><p>Остаток <b><%= technpassport.debt %></b></p></div>
    </div>
  <% end %>
</ul>